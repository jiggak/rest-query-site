<!doctype html>
<html lang="en">
<head>
   <meta charset="utf-8">
   <meta name="viewport" content="width=device-width, initial-scale=1">
   <title>Rest Query</title>
</head>
<body>
   <div class="container py-4 px-3 mx-auto">
      <div class="card">
         <div class="card-header">
            <button class="btn btn-primary btn-sm exec-button">Execute</button>
         </div>
         <div class="card-body">
            <div class="query-editor border"></div>
         </div>
      </div>
      <div class="results"></div>
   </div>
   <script id="results-template" type="text/x-handlebars-template">
      <table class="table table-striped">
         <thead>
            <tr>
            {{#each col}}
               <th>{{this}}</th>
            {{/each}}
            </tr>
         </thead>
         <tbody>
         {{#each data}}
            <tr>
               {{#each ../col}}
               <td>{{lookup ../this this}}</td>
               {{/each}}
            </tr>
         {{/each}}
         </tbody>
      </table>
   </script>
</body>
</html>